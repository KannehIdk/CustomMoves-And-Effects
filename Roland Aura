                if b:IsA("ParticleEmitter") then
                    b:Emit(2)
                end
            end
        end
    end
end

d("Part")

end)

delay(22, function()

    local p = game.Players.LocalPlayer
local c = p.Character
local h = c:FindFirstChild("HumanoidRootPart")

-- Function to set the color sequence for particle emitters
local function setColorSequence(object, color)
    for _, descendant in ipairs(object:GetDescendants()) do
        if descendant:IsA("ParticleEmitter") then
            descendant.Color = ColorSequence.new(color)
        end
    end
end

local function d(e)
    local f = game.ReplicatedStorage.Resources.Sunrise.AtomicSlash[e]
    local g = f:Clone()
    g.Parent = workspace
    g.CFrame = h.CFrame * CFrame.new(0, 0, 0) * CFrame.Angles(0, math.pi / 0.1, 0)

    -- Set the particle color before emitting
    setColorSequence(g, Color3.fromRGB(0,0,0)) -- Example: Purple color

    for _, a in ipairs(g:GetDescendants()) do
        if a:IsA("Attachment") then
            for _, b in ipairs(a:GetChildren()) do
                if b:IsA("ParticleEmitter") then
                    b:Emit(2)
                end
            end
        end
    end
end

d("Part")

end)

